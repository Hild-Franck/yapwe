{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
  },
  "plugins": [
    "react-redux"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-redux/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "rules": {
    "linebreak-style": [
        "error",
        "unix"
    ],
    "semi": [
        "error",
        "never"
    ],
    "no-console": "error"
  }
}